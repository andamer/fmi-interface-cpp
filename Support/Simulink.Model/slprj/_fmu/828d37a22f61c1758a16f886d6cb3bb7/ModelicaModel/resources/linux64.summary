CPPFLAGS=-I'/usr/bin/../include/omc/c/fmi' -DOMC_FMI_RUNTIME=1 -DCMINPACK_NO_DLL=1

override CPPFLAGS += 

CFILES = FmiTest_Model.c \
         FmiTest_Model_functions.c \
         FmiTest_Model_records.c \
         FmiTest_Model_01exo.c \
