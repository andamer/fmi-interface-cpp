CC=clang
AR=@AR@
CFLAGS=-Os -fPIC -falign-functions -mfpmath=sse -fno-dollars-in-identifiers 
LD=$(CC) -shared
LDFLAGS=-L'/usr/bin/../lib/x86_64-linux-gnu/omc' -Wl,-rpath,'/usr/bin/../lib/x86_64-linux-gnu/omc'   -lSimulationRuntimeFMI  -Wl,--no-as-needed -Wl,--disable-new-dtags -llapack -lblas -lm -lm -lpthread -rdynamic -Wl,--no-undefined 
DLLEXT=.so
NEED_RUNTIME=
NEED_DGESV=
